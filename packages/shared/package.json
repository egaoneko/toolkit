{
  "name": "@toolkit/shared",
  "version": "0.0.1",
  "description": "shared",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist",
    "types.d.ts"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "types": "tsc --project tsconfig.types.json",
    "prebuild": "yarn run clean && yarn run types",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\" --fix",
    "lint-style": "stylelint 'src/**/*.{css}' --allow-empty-input --fix",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx}\"",
    "preschema": "rimraf ./src/schemas",
    "schema": "node scripts/schema.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@toolkit/util": "0.0.1",
    "clsx": "1.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "react-transition-group": "4.4.2",
    "react-use": "17.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "6.5.9",
    "@toolkit/config": "0.0.1",
    "@toolkit/schema": "0.0.1",
    "storybook-dark-mode": "1.1.0"
  }
}
